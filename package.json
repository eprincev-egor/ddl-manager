{
  "name": "ddl-manager",
  "version": "2.0.40-alpha",
  "description": "store postgres procedures and triggers in files",
  "main": "dist/DDLManager.js",
  "bin": {
    "ddl-manager": "./bin/index.js"
  },
  "types": "lib/DDLManager.ts",
  "directories": {
    "dist": "dist",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "clear-dist": "rm -r ./dist || true",
    "copy-files": "npm run clear-dist && mkdir dist && cp -r ./lib/* ./dist",
    "build": "npm run copy-files && tsc --project publish-tsconfig.json",
    "test": "nyc --reporter=html --reporter=text-summary mocha",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eprincev-egor/ddl-manager.git"
  },
  "keywords": [
    "postgres",
    "ddl",
    "sql",
    "trigger",
    "procedure",
    "store",
    "parser",
    "migration"
  ],
  "author": "Eprincev Egor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eprincev-egor/ddl-manager/issues"
  },
  "homepage": "https://github.com/eprincev-egor/ddl-manager#readme",
  "dependencies": {
    "glob": "^7.1.3",
    "grapeql-lang": "^2.10.2",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "node-watch": "^0.6.0",
    "pg": "^7.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.3",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.3",
    "@types/node": "^12.12.18",
    "@types/pg": "^7.14.6",
    "chai": "^4.2.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "fs-extra": "^9.0.1",
    "mocha": "^5.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  }
}
