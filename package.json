{
  "name": "ddl-manager",
  "version": "1.0.13",
  "description": "store postgres procedures and triggers in files",
  "main": "dist/DDLManager.js",
  "types": "lib/DDLManager.ts",
  "directories": {
    "dist": "dist",
    "lib": "lib",
    "test": "test"
  },
  "bin": {
    "ddl-manager": "./bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json 'lib/**/*.ts' && tslint -c test/tslint.json 'test/**/*.ts'",
    "test": "mocha",
    "coverage": "nyc mocha",
    "prepublishOnly": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eprincev-egor/ddl-manager.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true,
    "exclude": [
      "dist",
      "coverage"
    ]
  },
  "keywords": [
    "postgres",
    "ddl",
    "sql",
    "trigger",
    "procedure",
    "store",
    "parser",
    "migration"
  ],
  "author": "Eprincev Egor",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eprincev-egor/ddl-manager/issues"
  },
  "homepage": "https://github.com/eprincev-egor/ddl-manager#readme",
  "dependencies": {
    "grapeql-lang": "^2.2.5",
    "model-layer": "^2.7.13"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "12.6.9",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
